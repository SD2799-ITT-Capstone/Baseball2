
/*
fucntion $ to replace 
document.getElementById
*/
function $(el) { return document.getElementById(el); }





function BattingExpand()
{
	var pic = $('BattingExpImg').src;
	if (pic.indexOf('icon_small_minus.gif') != -1)
	{
		$('#BattingTable td:nth-child(8), #BattingTable th:nth-child(8), #BattingTable td:nth-child(9),#BattingTable th:nth-child(9),#BattingTable td:nth-child(10),#BattingTable th:nth-child(10),#BattingTable td:nth-child(11),#BattingTable th:nth-child(11),#BattingTable td:nth-child(12),#BattingTable th:nth-child(12),#BattingTable td:nth-child(13),#BattingTable th:nth-child(13),#BattingTable td:nth-child(14),#BattingTable th:nth-child(14),#BattingTable td:nth-child(15),#BattingTable th:nth-child(15),#BattingTable td:nth-child(16),#BattingTable th:nth-child(16),#BattingTable td:nth-child(17),#BattingTable th:nth-child(17),#BattingTable td:nth-child(18),#BattingTable th:nth-child(18),#BattingTable td:nth-child(19),#BattingTable th:nth-child(19),#BattingTable td:nth-child(20),#BattingTable th:nth-child(20)').hide();
		$('BattingExpImg').src  = '/images/icon_small_plus.gif';
	}	
	else if (pic.indexOf('icon_small_plus.gif') != -1)
	{
		$('#BattingTable td:nth-child(8), #BattingTable th:nth-child(8), #BattingTable td:nth-child(9),#BattingTable th:nth-child(9),#BattingTable td:nth-child(10),#BattingTable th:nth-child(10),#BattingTable td:nth-child(11),#BattingTable th:nth-child(11),#BattingTable td:nth-child(12),#BattingTable th:nth-child(12),#BattingTable td:nth-child(13),#BattingTable th:nth-child(13),#BattingTable td:nth-child(14),#BattingTable th:nth-child(14),#BattingTable td:nth-child(15),#BattingTable th:nth-child(15),#BattingTable td:nth-child(16),#BattingTable th:nth-child(16),#BattingTable td:nth-child(17),#BattingTable th:nth-child(17),#BattingTable td:nth-child(18),#BattingTable th:nth-child(18),#BattingTable td:nth-child(19),#BattingTable th:nth-child(19),#BattingTable td:nth-child(20),#BattingTable th:nth-child(20)').show();
		$('BattingExpImg').src  = '/images/icon_small_minus.gif';
	}
}



